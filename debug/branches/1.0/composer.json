{
  "name": "pollen-solutions/debug",
  "description": "Pollen Debug Component - Collection of debugging tools.",
  "homepage": "https://www.presstify.com/pollen-solutions/debug/",
  "keywords": [
    "presstify",
    "components",
    "debug"
  ],
  "type": "presstify-component",
  "time": "2020-04-17",
  "license": "MIT",
  "authors": [
    {
      "name": "Jordy Manner",
      "email": "jordy.manner@milkcreation.fr",
      "role": "Developper"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://composer.tigreblanc.fr/presstify/"
    }
  ],
  "require": {
    "php": ">=7.4",
    "composer/installers": "~1.0",
    "filp/whoops": "^2.9",
    "maximebf/debugbar": "^1.16",
    "pollen-solutions/http": "^1.0",
    "pollen-solutions/support": "^1.0",
    "symfony/error-handler": "^5.2",
    "symfony/var-dumper" : "^5.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "Pollen\\Debug\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Pollen\\Debug\\Test\\": "tests"
    }
  },
  "suggest": {
    "pollen-solutions/container" : "Pollen Container Component - PSR-11 ready Dependencies Injection Container"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  }
}
